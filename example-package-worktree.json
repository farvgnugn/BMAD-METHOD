{
  "name": "multi-agent-project",
  "scripts": {
    "dev": "vite serve --port ${DEV_PORT:-3000} --host",
    "dev:api": "node server.js --port ${API_PORT:-4000}",
    "build": "vite build --outDir ${BUILD_DIR:-dist}",
    "test": "jest --runInBand --testNamePattern=\"${AGENT_NAME:-.*}\"",
    "test:watch": "jest --watch --runInBand --testNamePattern=\"${AGENT_NAME:-.*}\"",
    "test:db": "NODE_ENV=test DB_NAME=testdb${TEST_DB_SUFFIX:-''} jest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "vite": "^4.0.0",
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.0.0",
    "typescript": "^4.9.0"
  }
}